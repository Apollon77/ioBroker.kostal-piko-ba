{
  "//HINTS": "https://github.com/ioBroker/ioBroker/wiki/Adapter-Development-Documentation#structure-of-io-packagejson",


  "common": {
    "name": "kostal-piko-ba",
    "version": "0.0.3",
    "title": "Kostal PikoBA",
    "titleLang": {
      "en": "Kostal PikoBA",
      "de": "Kostal PikoBA"
    },
    "desc": {
      "en": "Adapter for reading Kostal Piko BA data",
      "de": "Adapter zum lesen von Kostal Piko BA Daten"
    },
    "authors": [
      "Christian Hombach"
    ],
    "platform": "Javascript/Node.js",
    "mode": "daemon",
    "enabled": true,
    "license": "MIT",
    "loglevel": "debug",
    "readme": "https://github.com/hombach/iobroker.kostal-piko-ba/blob/master/README.md",
    "icon": "picoba.png",
    "extIcon": "https://raw.githubusercontent.com/hombach/iobroker.kostal-piko-ba/admin/picoba.png",
    "keywords": [ "Kostal", "Piko", "inverter" ],
    "type": "energy",
    "news": {
      "0.0.3": {
        "en": "training",
        "de": "Training"
      },
      "0.0.1": {
        "en": "initial release",
        "de": "ErstverÃ¶ffentlichung"
      }
    },
    "main": "main.js",
    "compact": true,
    "materialize": true,
    "materializeTab": true,
    "adminTab": {
      "singleton": true,
      "name": {
        "en": "Kostal-PikoBA",
        "de": "Kostal-PikoBA"
      },
      "link": "",
      "fa-icon": "info"
    },
    "supportCustoms": true,
    "dependencies": [
      {
        "js-controller": ">=1.4.2"
      }
    ]
  },
  

  "native": {
    "ipaddress": "",
    "option1": true
  },


  "objects": [],


  "instanceObjects": [
    {
      "_id": "live",
      "type": "channel",
      "common": {
        "name": "live data"
      },
      "native": {}
    },
    {
      "_id": "live.powerAC",
      "type": "state",
      "common": {
        "role": "value",
        "name": "GridOutputPower without battery charging",
        "type": "number",
        "unit": "%",
        "read": true,
        "write": false,
        "def": false
      },
      "native": {}
    },

    {
      "_id": "day",
      "type": "channel",
      "common": {
        "name": "daily data"
      },
      "native": {}
    },
    {
      "_id": "day.autarky",
      "type": "state",
      "common": {
        "role": "value",
        "name": "Degree of autarky per day",
        "type": "number",
        "unit": "%",
        "read": true,
        "write": false,
        "def": false
      },
      "native": {}
    },

    {
      "_id": "lifetime",
      "type": "channel",
      "common": {
        "name": "lifetime data"
      },
      "native": {}
    },
    {
      "_id": "lifetime.autarky",
      "type": "state",
      "common": {
        "role": "value",
        "name": "Degree of autarky per inverter lifetime",
        "type": "number",
        "unit": "%",
        "read": true,
        "write": false,
        "def": false
      },
      "native": {}
    }
  ]
}
